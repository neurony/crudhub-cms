{
    "name": "neurony/crudhub-cms",
    "description": "Laravel Admin Panel",
    "license": "mit",
    "homepage": "https://crudhub.com",
    "keywords": [
        "laravel",
        "admin panel",
        "cms",
        "content",
        "pages",
        "menus",
        "blocks"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zbiller\\CrudhubCms\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zbiller\\CrudhubCms\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zbiller\\CrudhubCms\\CrudhubCmsServiceProvider"
            ]
        }
    }
}
